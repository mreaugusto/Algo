1:Return the 2 number that added to each other results in the target sum
2:public static int[] twoNumberSum(int[] array, int targetSum) {
3:	Arrays.sort(array);
	int left = 0;
	int right = array.length - 1;
	while(left < right) {
		int currentSum = array[left] + array[right];
		if(currentSum == targetSum) return new int[] { array[left], array[right] };
		if(currentSum < targetSum) left++;
		else right--;
	}
	return new int[0];
}
